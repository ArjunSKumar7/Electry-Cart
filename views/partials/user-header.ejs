<header
  class="header-area header-style-3 header-height-2"
  style="background-color: whitesmoke"
>
  <div class="header-middle header-middle-ptb-1 d-none d-lg-block">
    <div class="container">
      <div class="header-wrap">
        <div class="logo logo-width-1">
          <h3>PHONE<span style="color: black">CART</span></h3>
        </div>

        <div class="header-right">
          <div class="search-style-2">
            <form action="/search" method="post">
              <input
                type="text"
                name="search"
                id="keyword"
                placeholder="Search for items..."
              />

                <button type="submit" style="display: none">Submit</button>
            </form>
          </div>
          <div class="header-action-right">
            <div class="header-action-2">
              <li class="dropdown nav-item" style="list-style: none">
                <a
                  data-bs-toggle="dropdown"
                  href="#"
                  id="dropdownAccount"
                  aria-expanded="false"
                >
                  <img
                    class="img-xs rounded-circle"
                    src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fcdn5.vectorstock.com%2Fi%2Fthumb-large%2F04%2F09%2Fuser-icon-vector-5770409.jpg&f=1&nofb=1&ipt=cd5c65b1050b3260c6c55079ce0a1e365d19e45e9afde176b8af5c31d2c198d5&ipo=images"
                    height="50rem"
                    width="50rem"
                    alt="User"
                /></a>
                <div
                  class="dropdown-menu dropdown-menu-end"
                  aria-labelledby="dropdownAccount"
                >
                  <% if(!locals.userSession){%>
                  <a class="dropdown-item" href="/login" style="color: black"
                    >Log In</a
                  >
                  <%} else{%>
                  <!-- <a class="dropdown-item" href="/profile/<%=locals.profileId%>"
                    >Edit Profile</a
                  > -->
                  <a class="dropdown-item" href="/logout" style="color: red">Log out</a>
                  <!-- <a class="dropdown-item" href="/profile/<%= locals.profileId%>" style="color: green">profile</a> -->
                  <a class="dropdown-item" href="/profile" style="color: green">profile</a>
                  <%} %>
                </div>
              </li>
              <div class="header-action-icon-2">
                <a href="/view_wishlist">
                  <img
                    class="svgInject"
                    alt="Evara"
                    src="assets/imgs/theme/icons/icon-heart.svg"
                  />
                  <% if(locals.userSession){%>
                  <span class="pro-count blue" id="wishcount"
                    ><%= locals.wishcount %></span
                  >
                  <%}%>
                </a>
              </div>
              <div class="header-action-icon-2"
              >
                <a class="mini-cart-icon" href="/viewcart">
                  <img
                    alt="Evara"
                    src="assets/imgs/theme/icons/icon-cart.svg"
                  />
                  <% if(locals.userSession){%>

                  <span class="pro-count blue" id="cart-count"
                    ><%= locals.cartcount %></span
                  >
                  <%}%>
                </a>
              </div>
            </div>
            <div></div>
          </div>




        </div>







      </div>
    </div>
  </div>
  <div class="header-bottom header-bottom-bg-color sticky-bar">
    <div class="container">
      <div
        class="header-wrap header-space-between position-relative main-nav d-flex justify-content-center"
      >
        <div class="logo logo-width-1 d-block d-lg-none">
          <h4>PHONE<span style="color: black">CART</span></h4>
          <a href="/"></a>
        </div>
        <div class="header-nav d-none d-lg-flex">
          <div
            class="main-menu main-menu-padding-1 main-menu-lh-2 d-none d-lg-block"
          >
            <nav>
              <ul>
                <li>
                  <!-- active aakanam -->
                  <a class="" href="/">Home <i class=""></i></a>
                </li>
                <li>
                  <a href="/shop">Shop <i class=""></i></a>
                </li>
                <li class="position-static">
                  <a href="/orders">Orders <i class=""></i></a>
                </li>
              </ul>
            </nav>
          </div>
        </div>

        <p class="mobile-promotion"></p>
        <div class="header-action-right d-block d-lg-none">
          <div class="header-action-2">
            <!-- <div class="header-action-icon-2">
              <a href="/view_wishlist">
                <img alt="Evara" src="assets/imgs/theme/icons/icon-heart.svg" />
                <% if(locals.userSession){%>
                <span class="pro-count blue" id="wishcount"
                  ><%= locals.wishcount %></span
                >
                <%}%>
              </a>
            </div> -->
            <div class="header-action-icon-2">
              <a class="mini-cart-icon" href="/viewcart">
                <img alt="Evara" src="assets/imgs/theme/icons/icon-cart.svg" />
                <% if(locals.userSession){%>

                <span class="pro-count blue" id="cart-count"
                  ><%= locals.cartcount %></span
                >
                <%}%>
              </a>
            </div>
            <div class="header-action-icon-2 d-block d-lg-none">
              <div class="burger-icon burger-icon-white">
                <span class="burger-icon-top"></span>
                <span class="burger-icon-mid"></span>
                <span class="burger-icon-bottom"></span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>
<div class="mobile-header-active mobile-header-wrapper-style">
  <div class="mobile-header-wrapper-inner">
    <div class="mobile-header-top">
      <div class="mobile-header-logo">
        <h4>PHONE<span style="color: black">CART</span></h4>
        <a href="/"></a>
      </div>
      <div
        class="mobile-menu-close close-style-wrap close-style-position-inherit"
      >
        <button class="close-style search-close">
          <i class="icon-top"></i>
          <i class="icon-bottom"></i>
        </button>
      </div>
    </div>
    <div class="mobile-header-content-area">
      <div class="mobile-search search-style-3 mobile-header-border">
        <form action="/search" method="post">
          <input
            type="text"
            name="search"
            id="keyword"
            placeholder="Search for items..."
          />

          <button type="submit" style="display: none">Submit</button>
        </form>
      </div>
      <div class="mobile-menu-wrap mobile-header-border">
        <!-- mobile menu start -->
        <nav>
          <ul class="mobile-menu">
            <li class="menu-item-has-children">
              <span class="menu-expand"></span><a href="/">Home</a>
            </li>
            <li class="menu-item-has-children">
              <span class="menu-expand"></span><a href="/shop">Shop</a>
            </li>
            <li class="menu-item-has-children">
              <span class="menu-expand"></span><a href="/order">Orders</a>
            </li>
          </ul>
        </nav>
        <!-- mobile menu end -->
      </div>
      <div class="mobile-header-info-wrap mobile-header-border">
        <div class="single-mobile-header-info mt-30">
          <% if(!locals.userSession){%>
            <a class="dropdown-item" href="/login" style="color: black"
              >Log In</a
            >
            <%} else{%>
            <!-- <a class="dropdown-item" href="/profile/<%=locals.profileId%>"
              >Edit Profile</a
            > -->
            <a class="dropdown-item" href="/logout" style="color: red">Log out</a>
            <!-- <a class="dropdown-item" href="/profile/<%= locals.profileId%>" style="color: green">profile</a> -->
            <a class="dropdown-item" href="/profile" style="color: green">profile</a>
            <%} %>
        </div>
      </div>
      <div class="mobile-social-icon">
        <h5 class="mb-15 text-grey-4">Follow Us</h5>
        <a href="#"
          ><img src="assets/imgs/theme/icons/icon-facebook.svg" alt=""
        /></a>
        <a href="#"
          ><img src="assets/imgs/theme/icons/icon-twitter.svg" alt=""
        /></a>
        <a href="#"
          ><img src="assets/imgs/theme/icons/icon-instagram.svg" alt=""
        /></a>
        <a href="#"
          ><img src="assets/imgs/theme/icons/icon-pinterest.svg" alt=""
        /></a>
        <a href="#"
          ><img src="assets/imgs/theme/icons/icon-youtube.svg" alt=""
        /></a>
      </div>
    </div>
  </div>
</div>
